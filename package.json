{
  "name": "root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shedevro/web-utils.git"
  },
  "scripts": {
    "start": "lerna run start --stream",
    "build": "lerna run build",
    "lint": "lerna run lint",
    "test": "jest",
    "changelog:root": "conventional-changelog -p angular -i CHANGELOG.md -r 0 -svu",
    "changelog:scopes": "lerna exec --concurrency 1 --stream -- 'conventional-changelog -p angular -i $PWD/CHANGELOG.md --commit-path $PWD -k $PWD/package.json -r 0 -svu'"
  },
  "dependencies": {
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@types/node": "^16.18.61",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.53.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.7.0",
    "lerna": "^4.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.4.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2"
  }
}
